{
  "name": "nexuscode",
  "version": "1.0.0",
  "description": "Customer Management System",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build-exe": "pkg . --targets node16-win-x64,node16-linux-x64,node16-macos-x64 --output app"
  },
  "bin": "index.js",
  "pkg": {
    "scripts": [
      "script.js"
    ],
    "assets": [
      "public/**/*",
      "database.db",
      "node_modules/sqlite3/lib/binding/**/*"
    ],
    "targets": [
      "node16-win-x64"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "fs-extra": "^11.2.0",
    "sqlite3": "^5.1.7"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
